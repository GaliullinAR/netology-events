{
    "env": {
        "browser": true,
        "es6": true,
        "jest": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "2018",
        "sourceType": "module"
    },
    "rules": {
        "no-restricted-syntax": [
            "error",
            "WithStatement"
        ],
        "no-unused-vars": "off",
        "no-plusplus": "off",
        "no-console": "off",
        "array-callback-return": "off",
        "consistent-return": "off",
        "import/extensions": "off"
    }
}
